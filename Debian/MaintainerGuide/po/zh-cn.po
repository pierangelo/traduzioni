#| "$ quilt push\n"
#| "$ quilt refresh\n"
#| "$ quilt header -e\n"
#| "... describe patch\n"
msgid ""
"$ dpkg-source -x gentoo_0.9.12.dsc\n"
"$ cd gentoo-0.9.12\n"
"$ dquilt import ../<replaceable>foo</replaceable>.patch\n"
"$ dquilt push\n"
"$ dquilt refresh\n"
"$ dquilt header -e\n"
