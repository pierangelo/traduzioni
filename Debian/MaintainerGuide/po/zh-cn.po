"<refentrytitle>dh_make</refentrytitle> <manvolnum>8</manvolnum> </"
"citerefentry>、<citerefentry> <refentrytitle>debhelper</refentrytitle> "
"<manvolnum>1</manvolnum> </citerefentry>、<filename>/usr/share/doc/debhelper/"
